<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Employee"
		class="sapUiPopupWithPadding"
		items="{/employeeSet}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			title="{empId}"
			description="{empName}" />
	</SelectDialog>
</core:FragmentDefinition>