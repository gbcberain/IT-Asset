<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Employee"
		class="sapUiPopupWithPadding"
		items="{/employeeSet}"
		search="_handleValueHelpSearchTo"
		confirm="_handleValueHelpCloseTo"
		cancel="_handleValueHelpCloseTo">
		<StandardListItem
			title="{empId}"
			description="{empName}" />
	</SelectDialog>
</core:FragmentDefinition>